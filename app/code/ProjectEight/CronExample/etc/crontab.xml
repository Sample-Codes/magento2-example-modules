<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="default">
        <job name="projecteight_cronexample_write_to_log" instance="ProjectEight\CronExample\Cron\WriteToLog" method="execute">
            <schedule>*/30 * * * *</schedule>
        </job>
        <job name="projecteight_cronexample_write_to_another_log" instance="ProjectEight\CronExample\Cron\WriteToAnotherLog" method="execute">
            <schedule>*/10 * * * *</schedule>
        </job>
    </group>
</config>
