<?xml version="1.0" ?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route method="POST" url="/V1/projecteight-persistencelayerexample/whatsit">
        <service class="ProjectEight\PersistenceLayerExample\Api\WhatsItRepositoryInterface" method="save"/>
        <resources>
            <resource ref="ProjectEight_PersistenceLayerExample::WhatsIt_save"/>
        </resources>
    </route>
    <route method="GET" url="/V1/projecteight-persistencelayerexample/search">
        <service class="ProjectEight\PersistenceLayerExample\Api\WhatsItRepositoryInterface" method="getList"/>
        <resources>
            <resource ref="ProjectEight_PersistenceLayerExample::WhatsIt_view"/>
        </resources>
    </route>
    <route method="GET" url="/V1/projecteight-persistencelayerexample/:whatsitId">
        <service class="ProjectEight\PersistenceLayerExample\Api\WhatsItRepositoryInterface" method="getById"/>
        <resources>
            <resource ref="ProjectEight_PersistenceLayerExample::WhatsIt_view"/>
        </resources>
    </route>
    <route method="PUT" url="/V1/projecteight-persistencelayerexample/:whatsitId">
        <service class="ProjectEight\PersistenceLayerExample\Api\WhatsItRepositoryInterface" method="save"/>
        <resources>
            <resource ref="ProjectEight_PersistenceLayerExample::WhatsIt_update"/>
        </resources>
    </route>
    <route method="DELETE" url="/V1/projecteight-persistencelayerexample/:whatsitId">
        <service class="ProjectEight\PersistenceLayerExample\Api\WhatsItRepositoryInterface" method="deleteById"/>
        <resources>
            <resource ref="ProjectEight_PersistenceLayerExample::WhatsIt_delete"/>
        </resources>
    </route>
</routes>
